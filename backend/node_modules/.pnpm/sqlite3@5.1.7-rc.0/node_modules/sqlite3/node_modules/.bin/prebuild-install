#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/music/backend/node_modules/.pnpm/prebuild-install@7.1.1/node_modules/prebuild-install/node_modules:/music/backend/node_modules/.pnpm/prebuild-install@7.1.1/node_modules:/music/backend/node_modules/.pnpm/node_modules:/music/backend/node_modules:/music/node_modules:/node_modules:/music/backend/node_modules/.pnpm/sqlite3@5.1.7-rc.0/node_modules/sqlite3/node_modules:/music/backend/node_modules/.pnpm/sqlite3@5.1.7-rc.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/music/backend/node_modules/.pnpm/prebuild-install@7.1.1/node_modules/prebuild-install/node_modules:/music/backend/node_modules/.pnpm/prebuild-install@7.1.1/node_modules:/music/backend/node_modules/.pnpm/node_modules:/music/backend/node_modules:/music/node_modules:/node_modules:/music/backend/node_modules/.pnpm/sqlite3@5.1.7-rc.0/node_modules/sqlite3/node_modules:/music/backend/node_modules/.pnpm/sqlite3@5.1.7-rc.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../prebuild-install@7.1.1/node_modules/prebuild-install/bin.js" "$@"
else
  exec node  "$basedir/../../../../../prebuild-install@7.1.1/node_modules/prebuild-install/bin.js" "$@"
fi
